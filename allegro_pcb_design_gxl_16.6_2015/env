# Unit:millimeter
set BGA1_27 = 1.27
set BGA1_0 = 1.0
set BGA0_8 = 0.8
#set BGA0_8 = 3.81


# Unit: mil
set BGA1_27 = 50

#set BGA1_0 = 25.5900
set BGA1_0 = 39.37


set BGA0_8 = 31.50
#set BGA0_8 = 150

alias Up     ipick 0  $BGA1_27
alias Down   ipick 0 -$BGA1_27
alias Left   ipick   -$BGA1_27 0
alias Right  ipick    $BGA1_27 0

alias CUp    ipick 0  $BGA1_0
alias CDown  ipick 0 -$BGA1_0
alias CLeft  ipick   -$BGA1_0 0
alias CRight ipick    $BGA1_0 0

alias SUp    ipick 0  $BGA0_8
alias SDown  ipick 0 -$BGA0_8
alias SLeft  ipick   -$BGA0_8 0
alias SRight ipick    $BGA0_8 0





